includes:
	- phpstan-baseline.neon

parameters:
	excludePaths:
		analyse:
			- src/Subscriber/
			- test/Fixture/
			- src/Extension.php

	inferPrivatePropertyTypeFromConstructor: true

	level: max

	paths:
		- src/
		- test/
		- .php-cs-fixer.php
		- rector.php

	tmpDir: .build/phpstan/

	treatPhpDocTypesAsCertain: false
