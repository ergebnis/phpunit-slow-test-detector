{
  "name": "ergebnis/phpunit-slow-test-detector",
  "type": "library",
  "description": "Provides facilities for detecting slow tests in phpunit/phpunit.",
  "keywords": [
    "phpunit",
    "slow",
    "test",
    "detector",
    "extension"
  ],
  "homepage": "https://github.com/ergebnis/phpunit-slow-test-detector",
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "phpunit/phpunit": "dev-event#ffd44c130 as 10.0.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.13.2",
    "ergebnis/data-provider": "^1.1.0",
    "ergebnis/license": "^1.2.0",
    "ergebnis/php-cs-fixer-config": "~4.1.0",
    "fakerphp/faker": "^1.19.0"
  },
  "config": {
    "platform": {
      "php": "8.0.12"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\PHPUnit\\SlowTestDetector\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\PHPUnit\\SlowTestDetector\\Test\\": "test/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/theseer/phpunit"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/ergebnis/phpunit-slow-test-detector/issues",
    "source": "https://github.com/ergebnis/phpunit-slow-test-detector"
  }
}
